  "dependencies": {
    "axios": "^0.19.2",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "http-proxy-middleware": "^0.21.0",
    "if-env": "^1.0.4",
    "mongodb": "^4.1.0",
    "mongoose": "^5.9.1",
    "react-pdf": "^4.1.0",
    "react-scrollspy": "^3.4.2"
